<?xml version="1.0" encoding="utf-8"?>
<com.android.camera.ui.ManualExposurePopup style="@style/SettingPopupWindow"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:orientation="vertical" android:clickable="true" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <FrameLayout android:layout_width="fill_parent" android:layout_height="wrap_content">
            <TextView android:id="@id/title" style="@style/PopupTitleText" />
        </FrameLayout>
        <include android:id="@id/manual_exposure_iso" layout="@layout/manual_exposure_seek_bar" />
        <include android:id="@id/manual_exposure_time" layout="@layout/manual_exposure_seek_bar" />
        <View style="@style/PopupTitleSeperator" />
    </LinearLayout>
</com.android.camera.ui.ManualExposurePopup>